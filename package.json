{
  "name": "alelo",
  "version": "0.0.1",
  "main": "src/app.js",
  "dependencies": {
    "bootstrap-slider": "^4.3.0",
    "classnames": "^2.1.3",
    "flux": "^2.0.1",
    "jquery": "^2.1.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "onchange": "^2.4.0",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.0.0",
    "react-dom": "^15.1.0",
    "react-slick": "^0.12.2",
    "slick-carousel": "^1.6.0"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "jest-cli": "^0.4.3",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "2.1.1"
  },
  "scripts": {
    "start": "watchify -o dist/bundle.js -v -d src/app.js",
    "language": "cp language/en.json dist/en.json",
    "language:watch": "onchange 'language/en.json' -v -- npm run language",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > dist/bundle.min.js --verbose",
    "test": "jest"
  },
  "author": "Curtis Wingert",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "jest": {
    "rootDir": "./src"
  }
